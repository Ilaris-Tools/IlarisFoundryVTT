{{log 'effekte' this}}
<div>
    <h2>Effekte</h2>
    {{#each effects as |effect|}}
    <a class="item-edit" data-itemclass="effect" data-itemid="{{effect._id}}">
        {{effect.name}}
    </a>
    {{#unless (eq effect.flags.ilaris.sourceType "vorteil")}}
    <a class="item-delete" data-itemclass="effect" data-itemid="{{effect._id}}">
        <i class="fas fa-trash"></i>
    </a>
    {{/unless}}
    &nbsp; | &nbsp; {{/each}}
</div>
